{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
   "globals": {
        "ReactDOM": false,
    },
  "rules": {
    "comma-dangle": 1,
    "quotes": [ 1, "double" ],
    "semi": ["error", "always"],
    "no-multiple-empty-lines": ["error", {"max": 2, "maxEOF": 1}],
    "no-multi-spaces": 2,
    "linebreak-style": ["error", "unix"],
    "no-undef": 1,
    "indent": 1,
    "global-strict": 0,
    "space-infix-ops": ["error", {"int32Hint": false}],
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}
